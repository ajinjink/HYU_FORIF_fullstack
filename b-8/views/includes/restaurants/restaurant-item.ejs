<li class="restaurant-item">
    <article>
      <h2><%= res.name %></h2>
      <div class="restaurant-meta">
        <p><%= res.cuisine %></p>
        <p><%= res.address %></p>
      </div>
      <p>
        <%= res.description %>
      </p>
      <div class="restaurant-actions">
        <a href="<%= res.website %>">View Website</a>
      </div>
    </article>
</li>